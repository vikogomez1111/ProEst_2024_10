// ENTRADAS 

int main(){
    // Declaración de variables locales
    int i,j,m;

    // Estos bucles o sentencias repetitivas, recorreran todas las tablas de multiplicar
    // Este primer bucle recorre el multiplicando

for (i Este segundo bucle recorre el multiplicador{
            printf("Tabla del %d\n", i);
            printf("--------------\n");

            for (j=1; j<=12 ; j++){
                    m=i*j;
                    printf("%d * % d = %d\n",i,j,m);
            }
   }

}
